{
  "name": "graphql-server",
  "version": "1.0.0",
  "description": "a graphql server made with apollo and express",
  "main": "index.js",
  "repository": "git@github.com:Rafaell416/graphql-server.git",
  "author": "Rafael Villarreal <rvillarreal416@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "nodemon index.js",
    "db:migrate": "knex migrate:latest --knexfile=db/knexfile.js",
    "db:seed": "knex seed:run --knexfile=db/knexfile.js",
    "knex": "knex --knexfile=db/knexfile.js"
  },
  "private": false,
  "dependencies": {
    "apollo-server-express": "^1.2.0",
    "body-parser": "^1.18.2",
    "casual": "^1.5.17",
    "express": "^4.16.2",
    "graphql": "^0.11.7",
    "graphql-tools": "^2.7.2",
    "knex": "^0.14.0",
    "objection": "^0.9.0",
    "sqlite3": "^3.1.13"
  },
  "devDependencies": {
    "nodemon": "^1.12.1"
  }
}
